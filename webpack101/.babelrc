{
  "presets": [
    ["env", {
      /*  which browser versions do I want to support, */
      "targets": {
        "browsers": [ 
          /* https://github.com/browserslist/browserslist */
          "> 1%",
          "last 2 versions"
        ]
      },
      "useBuiltIns": "entry"
    }],
    "stage-2",
    "react"
  ],
  "plugins": [
    "syntax-dynamic-import"
  ]
}